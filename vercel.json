{
  "version": 2,
  "builds": [
    {
      "src": "api/all-in-one.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/all-in-one.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}